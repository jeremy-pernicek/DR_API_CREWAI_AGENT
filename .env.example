# DataRobot Configuration
DATAROBOT_API_TOKEN=your_datarobot_api_token
DATAROBOT_ENDPOINT=https://app.datarobot.com/api/v2/

# LLM API Keys
ANTHROPIC_API_KEY=your_claude_api_key
OPENAI_API_KEY=your_openai_api_key
GOOGLE_API_KEY=your_gemini_api_key

# GitHub Configuration (for higher rate limits)
GITHUB_TOKEN=your_github_token

# LLM Models (via DataRobot Gateway)
PRIMARY_MODELS=azure/gpt-4o-2024-11-20,bedrock/anthropic.claude-opus-4-1-20250805-v1:0,vertex_ai/gemini-2.5-pro
BACKUP_MODELS=vertex_ai/claude-opus-4-1@20250805,vertex_ai/gemini-2.0-flash-001,vertex_ai/gemini-1.5-pro-002

# Scraping Configuration
SCRAPE_SCHEDULE=0 2 * * *  # Daily at 2 AM
MAX_SCRAPE_WORKERS=5
SCRAPE_TIMEOUT=30

# Search Configuration
BM25_WEIGHT=0.6
FAISS_WEIGHT=0.4
MAX_SEARCH_RESULTS=10

# Generation Configuration
PARALLEL_GENERATION=true
MAX_GENERATION_TIME=30
ENABLE_CACHING=true
CACHE_TTL=3600